
<h2> Upload Category Keywords file for SEO </h2>
<div class='file-upload-form'>
  <div class='form-fields'>

    <% form_tag categories_url, :multipart => true  do %>
      <div class='field-label'> Upload File  </div>
      <div class='field-elem'> <%= file_field 'upload', 'datafile' %> </div>
      <div class='field-elem'> <%= submit_tag "Upload", :class=>'button-class' %> </div>  
    <% end %>

    
  </div>
</div>
